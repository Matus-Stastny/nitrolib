{
  "presets": [
    "@babel/react",
    "@babel/flow",
    ["@babel/env", { "targets": { "node": "current" } }],
    ["@babel/stage-2", { "decoratorsLegacy": true }]
  ],
  "plugins": [
    "relay",
    "idx",
    ["styled-components", { "ssr": true }],
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}
