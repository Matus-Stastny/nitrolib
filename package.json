{
  "name": "nitro",
  "private": true,
  "version": "0.0.0",
  "description": "Frontend on steroids.",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "rimraf dist && gulp && run-p devserver webpack:dev",
    "devserver": "yarn babel && run-p run:dev babel:dev",
    "run": "node dist/server",
    "run:dev": "nodemon dist/server",
    "render": "node dist/server/render",
    "bundle": "rimraf data dist && gulp && npm run babel && npm run webpack && npm run render",
    "babel": "babel src -d dist --ignore '**/__tests__/*'",
    "babel:dev": "babel src -d dist --ignore '**/__tests__/*' --watch --source-maps inline",
    "webpack": "webpack --config etc/webpack.build.js --mode production",
    "webpack:dev": "webpack-dev-server --config etc/webpack.dev.js --mode development --inline --hot",
    "relay": "relay-compiler --src ./ --include 'src/**' 'stories/**' --schema schema.graphql --extensions jsx js",
    "relay:schema": "graphql get-schema",
    "public": "rimraf public/lib && babel src/public -d public/lib --ignore '**/__tests__/*'",
    "test": "jest",
    "lint": "eslint './**/*.{js,jsx}'",
    "madge": "madge -c src/server/index.js src/client/index.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o dist-storybook"
  },
  "author": "kiwicom",
  "dependencies": {
    "@kiwicom/orbit-components": "^0.0.0-rc8",
    "date-fns": "^2.0.0-alpha.7",
    "fs-extra": "^6.0.1",
    "idx": "^2.3.0",
    "js-cookie": "^2.2.0",
    "koa": "^2.5.1",
    "koa-logger": "^3.2.0",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.1.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-icons": "^2.2.7",
    "react-relay": "^1.6.0",
    "react-router-dom": "^4.2.2",
    "relay-runtime": "^1.6.0",
    "styled-components": "^3.3.3",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-flow": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@babel/preset-stage-2": "^7.0.0-beta.52",
    "@storybook/addon-actions": "4.0.0-alpha.2",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "4.0.0-alpha.2",
    "@storybook/addons": "4.0.0-alpha.2",
    "@storybook/react": "4.0.0-alpha.2",
    "assets-webpack-plugin": "^3.8.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.4.0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-idx": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-airbnb": "^2.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.76.0",
    "graphql-cli": "^2.16.1",
    "gulp": "^4.0.0",
    "i18n-extract": "^0.6.0",
    "i18n-webpack-plugin": "^1.0.0",
    "jest": "^23.4.0",
    "jest-styled-components": "^5.0.1",
    "madge": "^3.2.0",
    "nodemon": "^1.18.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "raf": "^3.4.0",
    "reactizer": "^0.4.1",
    "relay-compiler": "^1.6.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
