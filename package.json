{
  "name": "@kiwicom/nitro",
  "version": "0.24.0",
  "description": "Nitro's components.",
  "scripts": {
    "start": "yarn storybook",
    "build": "yarn modules && run-p babel types keys",
    "babel": "babel src -d lib --ignore '**/__tests__/*'",
    "modules": "cross-env BABEL_ENV=modules babel src -d lib --ignore '**/__tests__/*' && renamer --find .js --replace .mjs 'lib/**'",
    "types": "flow-copy-source src lib --ignore '**/__tests__/*','**/__generated__/*' && renamer --find .jsx.flow --replace .js.flow 'lib/**'",
    "keys": "node etc/collectKeys.js",
    "relay": "relay-compiler --src src/ --schema schema.graphql --extensions jsx js",
    "relay:schema": "graphql get-schema",
    "test": "jest",
    "lint": "eslint './**/*.{js,jsx,js.flow}'",
    "typecheck": "flow check",
    "fixtures": "node ./bin/nitro.js keys 'src/**/*.{js,jsx}' && node ./bin/nitro.js fetch && node ./etc/fetchFixtures.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o dist-storybook",
    "prepare": "rimraf lib && run-p test lint typecheck && yarn build"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "kiwicom",
  "files": [
    "bin",
    "lib",
    "types",
    ".flowconfig",
    "tkeys.json",
    "README.md"
  ],
  "bin": {
    "nitro": "./bin/nitro.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@kiwicom/orbit-design-tokens": "^0.1.4",
    "chalk": "^2.4.1",
    "date-fns": "^2.0.0-alpha.7",
    "fs-extra": "^7.0.0",
    "i18n-extract": "^0.6.1",
    "idx": "^2.4.0",
    "mailcheck": "^1.1.1",
    "node-fetch": "^2.2.0",
    "prop-types": "^15.6.2",
    "react-transition-group": "^2.4.0",
    "validator": "^10.7.0"
  },
  "peerDependencies": {
    "@kiwicom/orbit-components": "^0.8.0",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-icons": "^2.2.7",
    "react-relay": "^1.6.2",
    "relay-runtime": "^1.6.2",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@kiwicom/orbit-components": "^0.8.0",
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-info": "^4.0.0-alpha.14",
    "@storybook/addon-knobs": "^4.0.0-alpha.14",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addons": "^4.0.0-alpha.14",
    "@storybook/react": "^4.0.0-alpha.14",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-id": "^1.0.1",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-relay": "^1.6.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-airbnb": "^2.5.3",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.2.0",
    "date-fns": "^2.0.0-alpha.7",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^2.0.2",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.5",
    "gulp": "^4.0.0",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.3",
