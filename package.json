{
  "name": "@kiwicom/nitro",
  "version": "0.0.3",
  "description": "Nitro's components.",
  "scripts": {
    "start": "yarn storybook",
    "build": "run-p babel types",
    "babel": "babel src -d lib --ignore '**/__tests__/*'",
    "types": "copyfiles -u 1 'src/**/*.js.flow' lib",
    "relay": "relay-compiler --src src/ --schema schema.graphql --extensions jsx js",
    "relay:schema": "graphql get-schema",
    "test": "jest",
    "lint": "eslint './**/*.{js,jsx,js.flow}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o dist-storybook",
    "prepare": "rimraf lib && yarn test && yarn lint && yarn flow check && yarn build"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "kiwicom",
  "files": [
    "bin",
    "lib",
    "src",
    "README.md"
  ],
  "bin": {
    "nitro": "./bin/nitro.js"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "date-fns": "^2.0.0-alpha.7",
    "fs-extra": "^6.0.1",
    "idx": "^2.3.0",
    "node-fetch": "^2.1.2",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-icons": "^2.2.7",
    "react-relay": "^1.6.0",
    "relay-runtime": "^1.6.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-flow": "^7.0.0-beta.52",
    "@babel/preset-react": "^7.0.0-beta.52",
    "@babel/preset-stage-2": "^7.0.0-beta.52",
    "@storybook/addon-actions": "4.0.0-alpha.2",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "4.0.0-alpha.2",
    "@storybook/addons": "4.0.0-alpha.2",
    "@storybook/react": "4.0.0-alpha.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.4.0",
    "babel-plugin-id": "^1.0.0",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-airbnb": "^2.5.1",
    "copyfiles": "^2.0.0",
    "date-fns": "^2.0.0-alpha.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.76.0",
    "graphql": "^0.13.2",
    "gulp": "^4.0.0",
    "jest": "^23.4.0",
    "jest-styled-components": "^5.0.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-icons": "^2.2.7",
    "react-relay": "^1.6.0",
    "relay-runtime": "^1.6.0",
    "rimraf": "^2.6.2",
    "styled-components": "^3.2.5"
  }
}
